{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title":"common",
  "definitions": {
    "@type": {
      "type": "string"
    },
    "Gender": {
      "type": "string",
      "enum": [
        "MALE",
        "FEMALE",
        "OTHER"
      ]
    },
    "Date": {
      "type": "string",
      "format": "date",
      "$comment": "YYYY-MM-DD format",
      "examples": [
        "1990-12-01"
      ]
    },
    "Year": {
      "type": "string",
      "pattern": "(^[0-9]{4}$)"
    }
  }
}