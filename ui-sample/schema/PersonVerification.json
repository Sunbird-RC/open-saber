{
  "$id": "#/properties/PersonVerified",
  "type": "object",
  "title": "The PersonVerified Schema",
  "required": [
    "personId"
  ],
  "additionalProperties": false,
  "properties": {
    "personId": {
      "$id": "#/properties/personId",
      "type": "string",
      "title": "personId"
    },
    "nameVerified": {
      "$id": "#/properties/nameVerified",
      "type": "string",
      "title": "Name"
    },
    "phonenameVerified": {
      "$id": "#/properties/phonenameVerified",
      "format": "phone",
      "title": "phone"
    },
    "teacherIdnameVerified": {
      "$id": "#/properties/teacherIdnameVerified",
      "type": "string",
      "$comment": "teacherId",
      "pattern": "^[0-9]{3}-.*",
      "title": "Teacher Id",
      "examples": [
        "888-abc",
        "639-def"
      ]
    },
     "schoolnameVerified": {
      "$id": "#/properties/schoolnameVerified",
      "type": "string",
      "title": "school"
    }
  }
}

