"Person": {
  "$id": "#/properties/Person",
  "type": "object",
  "title": "The Person Schema",
  "required": [
    "voterId",
    "name",
    "gender",
    "dob",
    "email",
    "phone"
  ],
  "additionalProperties": false,
  "properties": {
    "signatures": {
      "$id": "#/properties/signatures",
      "type": "array",
      "items": {
        "$ref": "Signature.json#/definitions/Signature"
      },
      "$comment": "Placeholder for all verifiable claims"
    },
     "photo": {
      "type": "string",
      "contentEncoding": "base64",
      "mediaType": "image/png"
    },
    "voterId": {
      "$id": "#/properties/voterId",
      "type": "string",
      "$comment": "Voter identifier"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "Full name"
    },
    "gender": {
      "$id": "#/properties/gender",
      "$ref": "Common.json#/definitions/Gender"
    },
    "doB": {
      "$id": "#/properties/birthDate",
      "$ref": "Common.json#/definitions/Date"
    },
    "emailId": {
      "$id": "emailId",
      "type": "string",
      "title": "Email ID"
    },
    "school": {
      "$id": "school",
      "type": "string",
      "title": "School"
    },
    "phoneNo": {
      "$id": "phoneNo",
      "type": "string",
      "title": "Phone"
    },
    "district": {
      "$id": "district",
      "type": "string",
      "title": "District"
    },
    "certificate": {
      "$id": "certificate",
      "type": "string",
      "title": "Certificate"
    }